  {
   "cell_type": "code",
   "execution_count": 112,
   "metadata": {},
   "outputs": [],
   "source": [
    "x_trainfinal=[]\n",
    "for i in range(len(x_train)):\n",
    "    a=word_tokenize(x_train[i])\n",
    "    clean_words=[w for w in a if not w in stop]\n",
    "    x_trainfinal.append(clean_words)"
   ]
  },
  {
   "cell_type": "code",
